{
  "env": {
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "quotes": ["warn", "double"],
    "implicit-arrow-linebreak": "off",
    "no-confusing-arrow": "off",
    "quote-props": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "no-console": "off",
    "comma-dangle": "off"
  }
}
